#include<stdio.h>
 
int main()
{
  	int i, number;
 
  	printf("\n Please Enter the final number to look up the odd numbers:  ");
  	scanf("%d", &number);
  
  	printf("\n Odd Numbers between 1 and %d are: \n", number);
  	for(i = 1; i <= number; i++)
      for ( i = 1 ; i <= 5; i++ ) 
  {
      for ( i = 1 ; i <= i; i++ ) 
    	if ( i % 2 != 0 ) 
    	{
  			printf(" %d\t", i);
    	}
  	}
 
  	return 0;
}
